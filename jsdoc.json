{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.(js|jsx)$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/js",
    "recurse": true,
    "template": "node_modules/clean-jsdoc-theme"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}